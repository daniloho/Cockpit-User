{
  "welcomeFile": "/flpSandbox.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/destinations/AlfaCockpit/(.*)$",
      "target": "/$1",
      "authenticationType": "none",
      "destination": "AlfaCockpit",
      "csrfProtection": false
    },
    {
      "source": "^/resources/(.*)$",
      "target": "$1",
      "service": "ui5",
      "authenticationType": "none"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "$1",
      "service": "ui5",
      "authenticationType": "none"
    }
  ]
}
