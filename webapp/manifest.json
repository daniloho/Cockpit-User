{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.alfa.cockpit.cockpitusers",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": { "version": "0.0.1" },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.0",
      "toolsId": "82aa5fb2-d487-409e-b64b-91aecb1ce957"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/Catalog/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/mainService/metadata.xml",
          "annotations": []
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "Users-Display": {
          "semanticObject": "Users",
          "action": "Display",
          "title": "{{Users-Display.flpTitle}}",
          "subTitle": "{{Users-Display.flpSubtitle}}",
          "signature": { "parameters": {}, "additionalParameters": "allowed" }
        }
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": { "icon": "", "favIcon": "", "phone": "", "phone@2": "", "tablet": "", "tablet@2": "" },
    "deviceTypes": { "desktop": true, "tablet": true, "phone": true }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.141.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": { "compact": true, "cozy": true },

    "models": {
      "": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "mainService",
        "preload": true,
        "settings": { "useBatch": false }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": { "bundleName": "com.alfa.cockpit.cockpitusers.i18n.i18n" }
      }
    },

    "resources": {
      "css": [{ "uri": "css/style.css" }]
    },

    "rootView": {
      "viewName": "com.alfa.cockpit.cockpitusers.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    },

    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "viewPath": "com.alfa.cockpit.cockpitusers.view",
        "controlId": "fcl",
        "controlAggregation": "beginColumnPages",
        "async": true
      },
      "routes": [
        { "name": "users", "pattern": "", "target": ["UsersList"] },
        { "name": "userDetail", "pattern": "Users/{userId}", "target": ["UserDetail"] }
      ],
      "targets": {
        "UsersList":  { "type": "View", "name": "UsersList",  "controlAggregation": "beginColumnPages" },
        "UserDetail": { "type": "View", "name": "UserDetail", "controlAggregation": "midColumnPages" }
      }
    }
  }
}
