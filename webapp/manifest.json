{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.alfa.cockpit.cockpitusers",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": { "version": "0.0.1" },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.0",
      "toolsId": "82aa5fb2-d487-409e-b64b-91aecb1ce957"
    },
    "dataSources": {
      "mainService": {
        "uri": "/destinations/AlfaCockpit/odata/v2/Catalog/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "Users-Display": {
          "semanticObject": "Users",
          "action": "Display",
          "title": "{{Users-Display.flpTitle}}",
          "subTitle": "{{Users-Display.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": { "icon": "", "favIcon": "", "phone": "", "phone@2": "", "tablet": "", "tablet@2": "" },
    "deviceTypes": { "desktop": true, "tablet": true, "phone": true }
  },

  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.141.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.ui.table": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": { "compact": true, "cozy": true },

    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.alfa.cockpit.cockpitusers.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": { "useBatch": false, "refreshAfterChange": false }
      },
      "view": { "type": "sap.ui.model.json.JSONModel" }
    },

    "resources": {
      "css": [{ "uri": "css/style.css" }]
    },

    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "com.alfa.cockpit.cockpitusers.view",
        "controlId": "fcl",
        "controlAggregation": "beginColumnPages",
        "transition": "slide",
        "async": true
      },
      "routes": [
        {
          "name": "users",
          "pattern": "",
          "target": ["usersList"]
        },
        {
          "name": "userDetail",
          "pattern": "user/{userId}",
          "target": ["usersList", "userObject"]
        }
      ],
      "targets": {
        "usersList": {
          "name": "UsersList",
          "controlAggregation": "beginColumnPages"
        },
        "userObject": {
          "name": "UserObjectPage",
          "controlAggregation": "midColumnPages"
        }
      }
    },

    "rootView": {
      "viewName": "com.alfa.cockpit.cockpitusers.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
